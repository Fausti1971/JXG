<?xml version="1.0" encoding="ISO-8859-1"?>
<!--bei fehlendem Inhalt (aber vorhandenem Tag) wird Defaultwert benutzt:
	offset = 0 (dezimal, dei descriptions (variable Parameter) ist dies der Parameterindex bspw. "REVERB PARAMETER 1")
	byteCount = 1 (dezimal)
	byteType = MIDIBYTE, NIBBLE
	valueType = NUMBER, TEXT, BITMAP
	translator = translateToText, translateNot, translateToTextPlus1, translateDiv10, translateSub128Div10, translateMap
	translationMap = ""
	translationMapFilter = ""
	descriptionMapIndex = 0... (der Index des Descriptions in der MutableParameterMap)
	descriptionDependsOf = 0 (dezimal, Offset des die Description definierenden Parameters bspw. ReverbType...)
-->
<maps xmlns:xs="http://www.w3.org/2001/XMLSchema-instance" xs:noNamespaceSchemaLocation="XGParameterMaps.xsd">
	<map name="fx1_parameters">
		<parameter>
			<offset>0</offset>
			<longName>Reverb Program</longName>
			<shortName>RevPrg</shortName>
			<valueMin>0</valueMin>
			<valueMax>69</valueMax>
			<byteCount>2</byteCount>
			<byteType>MIDIBYTE</byteType>
			<valueType>NUMBER</valueType>
			<translator>translateMap</translator>
			<translationMap filter="reverb">fx_programs</translationMap>
		</parameter>
	</map>

	<map name="Hall 1,Hall 2,Room 1,Room 2,Room 3,Stage 1,Stage 2,Plate">
		<parameter>
			<offset>1</offset>
			<longName>Reverb Time</longName>
			<shortName>RevTime</shortName>
			<valueMin>0</valueMin>
			<valueMax>69</valueMax>
			<translator>translateMap</translator>
			<translationMap>reverb_time</translationMap>
		</parameter>
	</map>

	<map name="multipart_parameters">
		<parameter>
			<offset>0</offset>
			<longName>Element Reserve</longName>
			<shortName>ElmRsv</shortName>
			<valueMin>0</valueMin>
			<valueMax>32</valueMax>
			<byteCount>1</byteCount>
			<byteType>MIDIBYTE</byteType>
			<valueType>NUMBER</valueType>
			<translator>translateToText</translator>
		</parameter>
		<parameter>
			<offset>1</offset>
			<longName>Bank Select MSB</longName>
			<shortName>BnkMSB</shortName>
			<valueMin>0</valueMin>
			<valueMax>127</valueMax>
			<byteCount>1</byteCount>
			<byteType>MIDIBYTE</byteType>
			<valueType>NUMBER</valueType>
			<translator>translateToText</translator>
		</parameter>

		<parameter>
			<offset>4</offset>
			<longName>MIDI Channel</longName>
			<shortName>Ch</shortName>
			<valueMin>0</valueMin>
			<valueMax>127</valueMax>
			<byteCount>1</byteCount>
			<byteType>MIDIBYTE</byteType>
			<valueType>NUMBER</valueType>
			<translator>translateMap</translator>
			<translationMap>midi_channels</translationMap>
		</parameter>

		<parameter>
			<offset>9</offset>
			<longName>Detune</longName>
			<shortName>Tune</shortName>
			<valueMin>0</valueMin>
			<valueMax>255</valueMax>
			<byteCount>2</byteCount>
			<byteType>NIBBLE</byteType>
			<valueType>NUMBER</valueType>
			<translator>translateSub128Div10</translator>
		</parameter>

		<parameter>
			<offset>26</offset>
			<longName>Attack Time</longName>
			<shortName>Atck</shortName>
			<valueMin>0</valueMin>
			<valueMax>127</valueMax>
		</parameter>

	</map>
	
</maps>