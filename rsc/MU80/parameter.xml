<?xml version="1.0" encoding="UTF-8"?>
<tables xmlns:xs="http://www.w3.org/2001/XMLSchema-instance" xs:noNamespaceSchemaLocation="../xsd/parameter.xsd">

	<table name="parameters">
		<item id="volume" longName="Volume" shortName="Vol" translator="normal"/>
		<item id="transpose" longName="Transpose" shortName="Xpose" min="40" max="88" translator="sub64"/>
		<item id="attenuator" longName="Attenuator" shortName="Damp" translator="normal"/>
		<item id="reverb_type" longName="Reverb Program" shortName="Prog" translator="map" translationMap="fx_programs" translationMapFilter="reverb"/>
		<item id="insertion_type" longName="Insertion Program" shortName="Prog" translator="map" translationMap="fx_programs" translationMapFilter="insertion"/>
		<item id="effect_return" longName="Effect Return" shortName="FXRet"/>
		<item id="effect_pan" longName="Effect Panorama" shortName="FXPan" min="1" translator="map" translationMap="panorama"/>

		<item id="master_tune" longName="Master Tune" shortName="Tune" max="2047" translator="sub1024Div10" unit="cnt"/>
		<item id="reverb_time" longName="Reverb Time" shortName="Time" max="69" translator="map" translationMap="reverb_time" unit="s"/>
		<item id="insertion_part" longName="Insertion Part" shortName="Part"/>
	
		<item id="reverb_1" master="2/1/0" index="1"/>
		<item id="reverb_2" master="2/1/0" index="2"/>
		<item id="reverb_3" master="2/1/0" index="3"/>
		<item id="reverb_4" master="2/1/0" index="4"/>
		<item id="reverb_5" master="2/1/0" index="5"/>
		<item id="reverb_6" master="2/1/0" index="6"/>
		<item id="reverb_7" master="2/1/0" index="7"/>
		<item id="reverb_8" master="2/1/0" index="8"/>
		<item id="reverb_9" master="2/1/0" index="9"/>
		<item id="reverb_10" master="2/1/0" index="10"/>

		<item id="insertion_1" master="3/0-15/0-0" index="1"/>
		<item id="insertion_2" master="3/0-15/0-0" index="2"/>
		<item id="insertion_3" master="3/0-15/0-0" index="3"/>
		<item id="insertion_4" master="3/0-15/0-0" index="4"/>
		<item id="insertion_5" master="3/0-15/0-0" index="5"/>
		<item id="insertion_6" master="3/0-15/0-0" index="6"/>
		<item id="insertion_7" master="3/0-15/0-0" index="7"/>
		<item id="insertion_8" master="3/0-15/0-0" index="8"/>
		<item id="insertion_9" master="3/0-15/0-0" index="9"/>
		<item id="insertion_10" master="3/0-15/0-0" index="10"/>

		<item id="mp_elementReserve" longName="Element Reserve" shortName="ElRes" max="32"/>
		<item id="mp_bank_msb" longName="Bank Select MSB" shortName="bankM"/>
		<item id="mp_bank_lsb" longName="Bank Select LSB" shortName="bankL"/>
		<item id="mp_program" longName="Voice Program Number" shortName="prgNr"/>
		<item id="mp_midichannel" longName="Midi Channel" shortName="ch" translator="map" translationMap="midi_channels"/>

		<item id="ad_inputGain" longName="Input Gain" shortName="iGain" max="1"/>

		<item id="dr_pitch_coarse" longName="Pitch Coarse" shortName="Tune" translator="sub64"/>
	</table>
	<table name="fx_parameter" caption="set of links for mutable fx-parameters">
		<set msb="0" lsb="0" caption="no effect">
			<entry index="1" parameter="insertion_1"/>
		</set>
	</table>
</tables>