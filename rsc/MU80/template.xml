<?xml version="1.1" encoding="ISO-8859-1"?>

<templates xmlns:xs="http://www.w3.org/2001/XMLSchema-instance" xs:noNamespaceSchemaLocation="../xsd/template.xsd">
	<template id="system">
		<item type="frame" name="Master">
			<item type="knob" x="master_tune"/>
			<item type="knob" x="master_volume"/>
			<item type="knob" x="master_attenuator"/>
			<item type="slider" x="master_transpose"/>
		</item>
	</template>
	<template id="multipart">
		<item type="frame" name="Amp Envelope">
			<item type="knob" x="mp_ampenv_attack"/>
			<item type="envelope" y="mp_volume">
				<item type="env_point" x="0" y="127"/>
				<item type="env_point" x="mp_ampenv_attack" y="mp_volume"/>
				<item type="env_point" x="mp_ampenv_decay" y="mp_volume"/>
				<item type="env_point" x="mp_ampenv_release" y="0"/>
			</item>
		</item>
	</template>
</templates>
