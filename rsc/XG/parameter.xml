<?xml version="1.1" encoding="ISO-8859-1"?>
<!--bei fehlendem Inhalt (aber vorhandenem Tag) wird Defaultwert benutzt
	ein fehlendes addressfield wird durch das entsprechende parent-addressfield ersetzt
	ein vorhandenes opcode-addressfield überschreibt des entsprechende parent-addressfield, aber nur falls es dort enthalten ist

	validierung funktioniert mit StAX nicht, also auch nicht das setzen eventueller default-Attribute
-->

<modules xmlns:xs="http://www.w3.org/2001/XMLSchema-instance" xs:noNamespaceSchemaLocation="../xsd/parameter.xsd">

	<module id="system" name="Master" hi="0" mid="0">
		<bulk lo="0-6">
			<opcode id="master_tune" lo="0-3" byteType="LSN">
				<parameter longName="Master Tune" shortName="Tune" max="2047" translator="translateSub128Div10"/>
			</opcode>
		</bulk>
	</module>

	<module id="sysfx" name="System Effect" hi="2" mid="1">
		<bulk lo="0-13">
			<opcode id="reverb_type" lo="0-1" byteType="LSB">
				<parameter longName="Reverb Program" shortName="Prog" translator="translateMap" translationMap="fx_programs" translationMapFilter="reverb"/>
			</opcode>
			<opcode id="reverb_1" lo="2" dependingOn="reverb_type">
				<mutableParameter hi="2" mid="1" lo="0" index="1"/>
			</opcode>
			<opcode id="reverb_2" lo="3" dependingOn="reverb_type">
				<parameter dependencyValues="1,2,3,4,16,17,18,19" longName="Diffusion" shortName="Diff" max="10" translator="translatePercent"/>
			</opcode>
			<opcode id="reverb_3" lo="4" dependingOn="reverb_type">
				<parameter dependencyValues="1,2,3,4,16,17,18,19" longName="Initial Delay" shortName="Delay" max="63" translator="translateMap" translationMap="delay_time_400"/>
			</opcode>
			<opcode id="reverb_4" lo="5" dependingOn="reverb_type">
				<parameter dependencyValues="1,2,3,4,16,17,18,19" longName="HPF Cutoff" shortName="HPF" max="52" translator="translateMap" translationMap="filter_freq"/>
			</opcode>
			<opcode id="reverb_5" lo="6" dependingOn="reverb_type">
				<parameter dependencyValues="1,2,3,4,16,17,18,19" longName="Reverb Time" shortName="RevTm" min="34" max="60" translator="translateMap" translationMap="filter_freq"/>
			</opcode>
			<opcode id="reverb_6" lo="7" dependingOn="reverb_type">
				<parameter dependencyValues="16,17,18,19" longName="Width" shortName="Width" max="37" translator="translateMap" translationMap="reverb_dimension"/>
			</opcode>
			<opcode id="reverb_7" lo="8" dependingOn="reverb_type">
				<parameter dependencyValues="16,17,18,19" longName="Height" shortName="Hight" max="73" translator="translateMap" translationMap="reverb_dimension"/>
			</opcode>
			<opcode id="reverb_8" lo="9" dependingOn="reverb_type">
				<parameter dependencyValues="16,17,18,19" longName="Depth" shortName="Depth" max="104" translator="translateMap" translationMap="reverb_dimension"/>
			</opcode>
			<opcode id="reverb_9" lo="10" dependingOn="reverb_type">
				<parameter dependencyValues="16,17,18,19" longName="Wall Vary" shortName="Wall" max="30"/>
			</opcode>
			<opcode id="reverb_10" lo="11" dependingOn="reverb_type">
				<parameter dependencyValues="1,2,3,4,16,17,18,19" longName="Dry/Wet" shortName="Wet" min="1" translator="translatePercent"/>
			</opcode>
			<opcode id="reverb_return" lo="12">
				<parameter longName="Reverb Return" shortName="Retrn"/>
			</opcode>
			<opcode id="reverb_pan" lo="13">
				<parameter longName="Reverb Panorama" shortName="Pan" min="1" translator="translateMap" translationMap="reverb_dimension"/>
			</opcode>
		</bulk>
	</module>

	<module id="insfx" name="Insertion Effect" hi="3" mid="0-15">
		<bulk lo="0-13">
			<opcode id="ins_type" lo="0-1" byteType="LSB">
				<parameter longName="Insertion Program" shortName="Prog" max="69" translator="translateMap" translationMap="fx_programs"/>
			</opcode>
			<opcode id="ins_1" lo="2" dependingOn="ins_type">
				<parameter dependencyValues="1,2,3,4,16,17,18,19" longName="Reverb Time" shortName="RevTm" max="69" translator="translateMap" translationMap="reverb_time"/>
			</opcode>
			<opcode id="ins_2" lo="3" dependingOn="ins_type">
				<parameter dependencyValues="1,2,3,4,16,17,18,19" longName="Diffusion" shortName="Diff" max="10" translator="translatePercent"/>
			</opcode>
			<opcode id="ins_3" lo="4" dependingOn="ins_type">
				<parameter dependencyValues="1,2,3,4,16,17,18,19" longName="Initial Delay" shortName="Delay" max="63" translator="translateMap" translationMap="delay_time_400"/>
			</opcode>
			<opcode id="ins_4" lo="5" dependingOn="ins_type">
				<parameter dependencyValues="1,2,3,4,16,17,18,19" longName="HPF Cutoff" shortName="HPF" max="52" translator="translateMap" translationMap="filter_freq"/>
			</opcode>
			<opcode id="ins_5" lo="6" dependingOn="ins_type">
				<parameter dependencyValues="1,2,3,4,16,17,18,19" longName="Reverb Time" shortName="RevTm" min="34" max="60" translator="translateMap" translationMap="filter_freq"/>
			</opcode>
			<opcode id="ins_6" lo="7" dependingOn="ins_type">
				<parameter dependencyValues="16,17,18,19" longName="Width" shortName="Width" max="37" translator="translateMap" translationMap="reverb_dimension"/>
			</opcode>
			<opcode id="ins_7" lo="8" dependingOn="ins_type">
				<parameter dependencyValues="16,17,18,19" longName="Height" shortName="Hight" max="73" translator="translateMap" translationMap="reverb_dimension"/>
			</opcode>
			<opcode id="ins_8" lo="9" dependingOn="ins_type">
				<parameter dependencyValues="16,17,18,19" longName="Depth" shortName="Depth" max="104" translator="translateMap" translationMap="reverb_dimension"/>
			</opcode>
			<opcode id="ins_9" lo="10" dependingOn="ins_type">
				<parameter dependencyValues="16,17,18,19" longName="Wall Vary" shortName="Wall" max="30"/>
			</opcode>
			<opcode id="ins_10" lo="11" dependingOn="ins_type">
				<parameter dependencyValues="1,2,3,4,16,17,18,19" longName="Dry/Wet" shortName="Wet" min="1" translator="translatePercent"/>
			</opcode>
			<opcode id="ins_part" lo="12">
				<parameter longName="Insertion Part" shortName="Part"/>
			</opcode>
		</bulk>
	</module>


	<module id="multipart" name="Multipart" hi="8-10" mid="0-31">
		<bulk hi="8" lo="0-40">
			<opcode id="mp_elementReserve" lo="0">
				<parameter longName="Element Reserve" shortName="ElRes" max="32"/>
			</opcode>
			<opcode lo="1" id="mp_bank_msb">
				<parameter longName="Bank Select MSB" shortName="bankM"/>
			</opcode>
			<opcode id="mp_bank_lsb" lo="2">
				<parameter longName="Bank Select LSB" shortName="bankL"/>
			</opcode>
			<opcode id="mp_program" lo="3">
				<parameter longName="Voice Program Number" shortName="prgNr"/>
			</opcode>
			<opcode id="mp_midichannel" lo="4">
				<parameter longName="Midi Channel" shortName="ch" translator="translateMap" translationMap="midi_channels"/>
			</opcode>
		</bulk>
	</module>

	<module id="adpart" name="A/D-Part" hi="16-18" mid="0-1">
		<bulk hi="16" mid="0-1" lo="0-30">
			<opcode id="ad_inputGain" hi="16" mid="0-1" lo="0">
				<parameter longName="Input Gain" shortName="iGain" max="1"/>
			</opcode>
		</bulk>
	</module>

	<module id="drumset" name="Drumset" hi="48-51" mid="13-91">
		<bulk hi="48-51" mid="13-91" lo="0-15">
			<opcode id="dr_pitch_coarse" hi="48-51" mid="13-91" lo="0">
				<parameter longName="Pitch Coarse" shortName="Tune" translator="translateSub64"/>
			</opcode>
		</bulk>
	</module>
</modules>