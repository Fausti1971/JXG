Parameter:
	ParameterTag tag
	int min, max
	String sName, lName
	Map table
	Translator translator
	XGAdress prg;
	int number;

	int getMinValue()
	{	return getParameter().min}

	getMaxValue();
	getLongName();
	getShortName();
	getValueAsText();

	private boolean isMutable()
	{	return this.adr != null;}

	private getParameter()
	{	if(this.isMutable) return XGParameterMaps.getParameter((int)XGObject.getValue(adr), nr);
		else return this;
	}
