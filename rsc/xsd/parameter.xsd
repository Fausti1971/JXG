<?xml version="1.0" encoding="ISO-8859-1"?>
<!-- xs: erforderlich, da "byteType" bereits anderweitig belegt ist -->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
	<xs:element name="map">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="parameter" minOccurs="0" maxOccurs="unbounded">
					<xs:complexType>
						<xs:all>
							<xs:element name="name" type="xs:string"/>
							<xs:element name="longName" type="xs:string" minOccurs="0"/>
							<xs:element name="shortName" type="xs:string" minOccurs="0"/>
							<xs:element name="valueMin" type="xs:nonNegativeInteger" minOccurs="0"/>
							<xs:element name="valueMax" type="xs:nonNegativeInteger" minOccurs="0"/>
							<xs:element name="translator" type="translatorType" minOccurs="0"/>
							<xs:element name="translationMap" minOccurs="0"/>
							<xs:element name="dependsOf" type="xs:string" minOccurs="0"/>
						</xs:all>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>

	<xs:simpleType name="translatorType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="translateToText"/>
			<xs:enumeration value="translateNot"/>
			<xs:enumeration value="translateToTextPlus1"/>
			<xs:enumeration value="translateDiv10"/>
			<xs:enumeration value="translateSub128Div10"/>
			<xs:enumeration value="translateMap"/>
		</xs:restriction>
	</xs:simpleType>

</xs:schema>