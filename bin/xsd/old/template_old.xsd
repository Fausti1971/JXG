<?xml version="1.0" encoding="ISO-8859-1"?>
<xs:schema elementFormDefault="qualified" xmlns="template" xmlns:t="template" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="t" xmlns:Q1="t">
	<xs:element name="templates" type="Q1:templates"></xs:element>

	<xs:complexType name="item">
		<xs:attribute name="id" use="optional" type="xs:ID"/>
		<xs:attribute name="control" use="required" type="Q1:controls"/>
	</xs:complexType>

	<xs:simpleType name="modules">
		<xs:restriction base="xs:NCName">
			<xs:enumeration value="system" />
			<xs:enumeration value="info" />
			<xs:enumeration value="sysfx" />
			<xs:enumeration value="syseq" />
			<xs:enumeration value="insfx" />
			<xs:enumeration value="display" />
			<xs:enumeration value="multipart" />
			<xs:enumeration value="adpart" />
			<xs:enumeration value="drumset" />
			<xs:enumeration value="plugin" />
			<xs:enumeration value="value"></xs:enumeration>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name = "controls">
		<xs:restriction base="xs:NCName">
			<xs:enumeration value="button"/>
			<xs:enumeration value="frame"/>
			<xs:enumeration value="knob"/>
			<xs:enumeration value="slider"/>
			<xs:enumeration value="doubleslider"/>
			<xs:enumeration value="combo"/>
			<xs:enumeration value="list"/>
			<xs:enumeration value="radio"/>
			<xs:enumeration value="checkbox"/>
			<xs:enumeration value="checkboxmenu"/>
		</xs:restriction>
	</xs:simpleType>


	<xs:element name="template" type="Q1:template"></xs:element>


	<xs:complexType name="template">
		<xs:sequence>
			<xs:element name="item" type="Q1:item" maxOccurs="unbounded" minOccurs="0"></xs:element>
		</xs:sequence>
		<xs:attribute ref="Q1:module"></xs:attribute>
	</xs:complexType>

	<xs:complexType name="templates"></xs:complexType>



    <xs:attribute name="module" type="Q1:modules"></xs:attribute>

    <xs:attribute name="control" type="Q1:controls"></xs:attribute>
</xs:schema>